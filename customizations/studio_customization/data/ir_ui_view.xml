  <record id="studio_customization.odoo_studio_project__c0b29c48-339e-43b5-a6f7-f0bb44721b44" model="ir.ui.view" context="{'studio': True}">
    <field name="active" eval="True"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='available_for_all_projects']" position="after">
          <field name="epic_responsible_ids">
            <tree editable="bottom">
              <field name="x_studio_sequence" widget="handle"/>
              <field optional="show" name="x_studio_epic_id" attrs="{}" invisible="1" required="1"/>
              <field optional="show" name="x_studio_project_id"/>
              <field name="x_studio_user_id" widget="many2one_avatar_user"/>
            </tree>
          </field>
        </xpath>
      </data>
    </field>
    <field name="field_parent" eval="False"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="inherit_id" ref="project_backlog_base.project_scrum_epic_form_view"/>
    <field name="key" eval="False"/>
    <field name="mode">extension</field>
    <field name="model">project.scrum.epic</field>
    <field name="name">Odoo Studio: project.scrum.epic.form.view customization</field>
    <field name="priority">99</field>
    <field name="type">form</field>
  </record>
</odoo>
